/**
 * Sample business network definition.
 */
namespace org.acme.sample

asset Message identified by messageId {
  o String messageId
  o String value
  --> User chatUser
}

participant User identified by facebookId {
  o String facebookId
}

transaction SendMessage {
  o String value
  --> User chatUser
}

event ShowChat {
  --> Message message
  --> User chatUser
  o String messageText
}